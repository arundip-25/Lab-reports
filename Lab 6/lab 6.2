.	# Base class Animal
	class Animal:
	# Method to print a general animal sound
	    def speak(self):
	        print("Animal makes a sound")
	
	# Derived class Dog inheriting from Animal
	class Dog(Animal):
  # Overriding the speak() method
	    def speak(self):
       print("Dog barks")
	
 # Instantiate the Dog class
	dog = Dog()
	
	# Call the speak() method of the Dog class
	dog.speak()
	
